<h1 class="text-center mb-3">{{title}}</h1>
<form action="/api/users" class="mb-4" method="POST">

<div class="form-group">
    <label for="name"> name</label>
    <input type="text" name='name'  class="form-control">
</div>
<div class="form-group">
    <label for="email"> email</label>
    <input type="email" name='email'  class="form-control">
</div>
<input type="submit" value="add member" class="btn  btn-primary btn-block">
</form>
<h4>members </h4>
<ul class="list-group">
  {{#each members}}
    <li class="list-groupt-item">{{this.name}} : {{this.email}}</li>
  {{/each}}

</ul>


<a href="/api/users" class="btn btn-light mt-4"> visit api</a>